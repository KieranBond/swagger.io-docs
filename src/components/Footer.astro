<script>
    document.addEventListener('DOMContentLoaded', function() {
        const productCategory = document.querySelector('.product-column-category');
        const productContent = document.querySelector('.product-column-content');
        if (!productCategory || !productContent) return;
        productCategory.addEventListener('click', function() {
            productContent.classList.toggle('active');
        });
    });
</script>