    }
    @media (max-width: 1439px) {
        .main-footer .right-side .contact {
            @apply mb-[15px];
        }
        .main-footer .right-side .contact ul {
            @apply text-center pl-0;
        }
    }
    .main-footer .right-side .contact span {
        @apply text-[#758491];
    }
    .main-footer .right-side .contact .call {
        @apply text-[#ff8f0f];
        font-family: 'Open Sans', sans-serif;
    }
    .main-footer .right-side .contact li {
        @apply px-0 py-[5px];
        font-family: 'Roboto Mono', monospace;
        list-style: none;
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .contact li {
            @apply inline-block text-center;
        }
    }
    @media (max-width: 546px) {
        .main-footer .right-side .contact li {
            @apply block;
        }
    }
    .main-footer .right-side .contact .vertical-line {
        @apply hidden text-[#eff0f0] px-[1%] py-0;
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .contact .vertical-line {
            @apply inline-block last-of-type:hidden;
        }
    }
    @media (max-width: 546px) {
        .main-footer .right-side .contact .vertical-line {
            @apply hidden;
        }
    }
    .main-footer .right-side hr {
        @apply border mx-0 my-5 border-solid border-[#758491];
    }
    .bottom-footer {
        @apply bg-[#231f20] px-0 py-[30px];
        margin-top: 0 !important;
    }
    .bottom-footer .bottom-links-container span {
