<style>
	.meta {
		gap: 0.75rem 3rem;
		justify-content: space-between;
		flex-wrap: wrap;
		margin-block: 3rem 1.5rem;
		font-size: var(--sl-text-sm);
		color: var(--sl-color-gray-3);
	}
	.meta > :global(p:only-child) {
		margin-inline-start: auto;
	}
</style>

<style>
    .main-footer {
        @apply bg-[#173647] pt-[60px] pb-[100px] px-0;
    }
    @media screen and (-ms-high-contrast: active),
        screen and (-ms-high-contrast: none) {
        .main-footer .gearbear {
            @apply bottom-[-131px];
        }
    }
    .main-footer .product-column {
        @apply isolate;
    }
    @media (max-width: 546px) {
        .main-footer .product-column {
            @apply text-center;
        }
    }
    .main-footer .product-column ul {
        @apply p-0;
    }
    @media (max-width: 546px) {
        .main-footer .product-column ul {
            @apply inline-block mx-auto my-0;
        }
    }
    .main-footer .product-column li {
        @apply leading-[50px];
        list-style: none;
    }
    .main-footer .product-column .display-5 {
        font-size: 1.1875rem;
        line-height: 2.078125rem;
        letter-spacing: 0.015em;

        @apply text-[#d2e1e8] font-light;
    }
    .main-footer .product-column a {
        @apply text-[#d2e1e8] text-base inline-block;
    }
    @media (max-width: 1199px) {
        .main-footer .product-column a {
            @apply text-[15px];
        }
    }
    .main-footer .product-column a.footer-external::after {
        @apply content-[""] font-light inline-block text-[10px] top-[-9px] text-[#758491] rotate-45 relative ml-[5px];
        font-family: 'Font Awesome 5 Pro';
    }
    .main-footer .product-column a .prod-icon {
        @apply h-[25px] w-[25px] mr-[15px] relative top-[6px];
    }
    @media (max-width: 1199px) {
        .main-footer .product-column a .prod-icon {
            @apply h-5 w-5;
        }
    }
    .main-footer .product-column hr {
        @apply border w-10 mx-0 my-5 border-solid border-[#758491];
    }
    .main-footer .product-column .sub-column {
        @apply float-left p-0;
    }
    .main-footer .product-column .product-column-category {
        @apply leading-10 text-lg;
        cursor: pointer;
    }
    .product-column-content li a,
    .bottom-links li a {
        @apply no-underline;
        &:hover {
            @apply underline;
        }
    }
    @media (max-width: 1199px) {
        .main-footer .product-column .product-column-content {
            @apply max-h-0 transition-all duration-[0.2s] ease-[ease-in-out] overflow-hidden;
        }
        .main-footer .product-column .product-column-content.active {
            @apply max-h-[1100px];
        }
    }
    .main-footer .right-side {
        @apply text-right border-l-[#758491] border-l border-solid;
        a {
            text-decoration: none;
        }
    }
    @media (max-width: 1439px) {
        .main-footer .right-side {
            @apply border-l-0;
        }
    }
    @media (max-width: 1029px) {
        .main-footer .right-side {
            @apply mt-[15px];
            text-align: initial;
        }
    }
    .main-footer .right-side .right-side-container {
        @apply float-left -mt-10;
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .right-side-container {
            @apply float-none;
        }
    }
    .main-footer .right-side li {
        @apply list-none;
    }
    .main-footer .right-side .more-links {
        @apply float-right;
    }
    .main-footer .right-side .more-links a {
        @apply text-[#ff8f0f];
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .more-links {
            @apply text-center;
        }
        .main-footer .right-side .more-links ul {
            @apply pl-0;
        }
    }
    .main-footer .right-side .more-links li {
        list-style: none;
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .more-links li {
            @apply inline;
        }
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .more-links hr {
            @apply hidden;
        }
    }
    .main-footer .right-side .more-links .vertical-line {
        @apply hidden text-[#eff0f0] px-[1%] py-0;
    }
    @media (max-width: 1439px) {
        .main-footer .right-side .more-links .vertical-line {
            @apply inline;
        }
    }
    .main-footer .right-side .contact {
